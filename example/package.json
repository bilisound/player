{
  "name": "bilisound-player-example",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start --dev-client -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "prepare": "npm run clean && expo prebuild",
    "clean": "node -e \"const opt = { recursive: true, force: true }; fs.rmSync('./ios', opt); fs.rmSync('./android', opt); fs.rmSync('./node_modules/.cache', opt)\""
  },
  "dependencies": {
    "axios": "^1.9.0",
    "md5": "^2.3.0",
    "promise-memoize": "^1.2.1",
    "react-native-logs": "^5.3.0",
    "expo": "^53.0.0",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "expo-build-properties": "~0.14.6",
    "react-dom": "19.0.0",
    "react-native-web": "~0.20.0",
    "@expo/metro-runtime": "~5.0.4",
    "expo-system-ui": "~5.0.7"
  },
  "devDependencies": {
    "@types/md5": "^2.3.5",
    "@types/promise-memoize": "^1.2.4",
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true,
  "expo": {
    "autolinking": {
      "nativeModulesDir": ".."
    }
  }
}
